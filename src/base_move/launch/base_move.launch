<launch>
    <node pkg = "rosserial_python" type = "serial_node.py" name = "serial_node">
        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="115200"/>
    </node>
    <node pkg = "base_move" type = "my_odometry" name = "my_odometry"/>
    <node pkg = "base_move" type = "my_navigation" name = "my_navigation"/>
    <node pkg = "base_move" type = "call_goal" name = "call_goal"/>
</launch>